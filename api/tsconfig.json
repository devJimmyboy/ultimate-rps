{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "moduleResolution": "node",
    "baseUrl": "./",
    "types": ["node"],
    "resolveJsonModule": true,
    "declaration": true,
    "sourceMap": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "skipLibCheck": false
  },
  "include": ["src/**/*"],
  "ts-node": {
    "files": true
  }
}
